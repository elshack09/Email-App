{
  "name": "server",
  "version": "1.0.0",
  "description": "Email App using Mongoose, Express, React, and Node with OAuth 2.0.",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "mongo": "^0.1.0",
    "mongoose": "^5.2.12",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "path": "^0.12.7",
    "stripe": "^6.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elshack09/Email-App.git"
  },
  "bugs": {
